{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "plugins": [
        "json"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:json/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
    "no-undef": "error",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "no-use-before-define": "warn"
    }
}
